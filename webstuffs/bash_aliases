alias gh='history|grep'
alias ve='python3 -m venv ./venv'
alias va='source ./venv/bin/activate'
alias tcn='mv --force -t ~/.local/share/Trash '
alias faceoff='cd /home/ubuntu/faceoff'
alias flaskrun='export FLASK_APP=server.py; flask run'
alias flaskenv='export FLASK_ENV=development'
alias flaskprod='export FLASK_ENV=production'
alias flaskstart='faceoff; conda activate faceoff'
alias gpuwork='faceoff; conda activate faceoff; rq worker gpu'
alias cpuwork='faceoff; conda activate detect; rq worker cpu'
alias restartng='sudo systemctl restart nginx'
alias startfaceoff='sudo systemctl start faceoff; sudo systemctl enable faceoff'
alias stopfaceoff='sudo systemctl stop faceoff'
alias statusfaceoff='sudo systemctl status faceoff'
